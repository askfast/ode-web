section#dashboard(style='margin-top: -20px;')
	.row
		#availabilities.sa.widget.col-md-12
			div.head
				h4
					| Aanwezig
			div(ng-show='loadingPresence')
				hr
				p Aanwezigheid aan het laden...
			section(ng-hide='loadingPresence')
				div(ng-repeat="group in present")
					hr
					ul.sa-setup.presence
						li.sa-truck
							.sa-member(ng-repeat='member in group')
								.media
									.pull-left
										.roundedPicMedium.memberStateNone(profile-img='{{member.uuid | avatar:"user":"100":member.resources.avatarPhotoId }}')
									.sa-member-card.media-body
										h5
											a(href='#/profile/{{member.uuid}}#profile')
												| {{member.name | toTitleCase}}
										hr
										span(ng-repeat='group in member.resources.groups') {{group.name}}